# Test db file for ads communication. 
# Macros and templates have been avoided in order to focus on showing the functionality.

record(ao,"$(P)SetFAmplitude"){
   field(PINI, "1")
   field(DTYP, "asynFloat64")
   field(OUT,  "@asyn($(PORT),0,1)ADSPORT=851/Main.fAmplitude")
   field(PREC, "3")
}

record(ao,"$(P)SetITestError"){
   field(PINI, "1")
   field(DTYP, "asynFloat64")
   field(OUT,  "@asyn($(PORT),0,1)ADSPORT=851/Main.iTestError")
   field(PREC, "3")
}

record(ai,"$(P)GetITest"){
   field(PINI, "1")
   field(DTYP, "asynFloat64")
   field(INP,  "@asyn($(PORT),0,1)ADSPORT=851/Main.iTestError")
   field(PREC, "3")
   field(SCAN,"I/O Intr")
}

