
record(bi,"$(P)Rotation-Brk-FreeWheelOn-Stat"){
  field(PINI, "1")
  field(TSE, -2)
  field(DTYP, "asynInt32")
  field(INP,  "@asyn($(PORT),0,1)ADSPORT=852/Main.bBrakeFreeWheelOn?")
  field(SCAN,"I/O Intr")
  field(ZNAM,"Operation Mode")
  field(ONAM,"Param Mode")
  field(DESC,"Rotation: Brake Freewheel On ")
}

