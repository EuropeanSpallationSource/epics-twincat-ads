# Records for monitoring of the Indra drive CMD structure (cmd can be find futher down)
record(ai,"$(P)Rotation-Drv-Cmd-OpMode-RB"){
  field(PINI, "1")
  field(TSE,  "-2")
  field(DTYP, "asynInt32")
  field(INP,  "@asyn($(PORT),0,1)ADSPORT=852/Main.stCommands.eOperationMode?")
  field(PREC, "0")
  field(SCAN, "I/O Intr")
  field(DESC, "Rot: Drive Op Mode Cmd RB")
}

