record(bo,"$(P)Rotation-Ena-Cmd"){
  field(PINI, "1")
  field(TSE,  "-2")
  field(DTYP, "asynInt32")
  field(OUT,  "@asyn($(PORT),0,1)ADSPORT=852/Main.bEnable=")
  field(SCAN, "Passive")
  field(ZNAM, "Zero")
  field(ONAM, "One")
  field(DESC, "Rotation: Drive Enable Cmd")
}

